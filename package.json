{
  "name": "vite-project",
  "private": true,
  "version": "0.0.0",
  "scripts": {
    "dev": "vite",
    "lint": "eslint --ext .vue,.ts src/",
    "lintfix": "npm run lint -- --fix",
    "format": "prettier --write \"**/*.+(js|json|yml|ts|vue)\"",
    "test": "jest",
    "build": "vue-tsc --noEmit && vite build",
    "deploy": "npm run build && gh-pages -ad dist",
    "preview": "vite preview"
  },
  "dependencies": {
    "bootstrap": "^5.1.3",
    "vue": "^3.2.25",
    "vue-router": "^4.0.16"
  },
  "devDependencies": {
    "@types/jest": "^28.1.1",
    "@typescript-eslint/eslint-plugin": "^5.28.0",
    "@typescript-eslint/parser": "^5.28.0",
    "@vitejs/plugin-vue": "^2.3.3",
    "@vue/eslint-config-prettier": "^7.0.0",
    "@vue/eslint-config-typescript": "^11.0.0",
    "@vue/test-utils": "^2.0.1",
    "eslint": "^8.17.0",
    "eslint-plugin-vue": "^9.1.1",
    "gh-pages": "^4.0.0",
    "jest": "^26.6.3",
    "prettier": "^2.7.0",
    "ts-jest": "^26.5.6",
    "typescript": "^4.5.4",
    "vite": "^2.9.9",
    "vue-jest": "^5.0.0-alpha.10",
    "vue-tsc": "^0.34.7"
  }
}
